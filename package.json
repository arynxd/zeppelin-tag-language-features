{
  "name": "zeppelin-tag-language-features",
  "displayName": "Zeppelin Tag Language Features",
  "description": "Provides language features for the zeppelin tag language",
  "repository": {
    "url": "https://github.com/arynxd/zeppelin-tag-language-features"
  },
  "icon": "media/icon.png",
  "publisher": "arynxd",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:zep"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "zep",
        "aliases": [
          "Zeppelin Tag",
          "zep"
        ],
        "extensions": [
          ".zep"
        ],
        "configuration": "./syntax/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zep",
        "scopeName": "text.plain.tag",
        "path": "./syntax/syntaxes/zep.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
